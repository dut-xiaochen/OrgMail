div.da_topPanel4Viewer {
	position: absolute;
	left: 0px;
	top: 0px;
	width: 100%;
	height: 53px;
	margin: 0px;
	padding: 0px;
	border: 0px;
}

div.da_topPanel4ViewerLeft {
	position: absolute;
	top: 0px;
	left: 0px;
	width: 0px;
	height: 100%;
	background-repeat: no-repeat;
	background-position: 0px 0px;
	z-index: 2;
}

div.da_topPanel4ViewerCenter {
	position: absolute;
	top: 0px;
	left: 0px;
	width: 99%;
	height: 100%;
	background-repeat: repeat-x;
	background-position: 0px 0px;
	z-index: 1;
}

div.da_topPanel4ViewerRight {
	position: absolute;
	top: 0px;
	right: 0px;
	width: 0px;
	height: 100%;
	background-repeat: no-repeat;
	background-position: 0px 0px;
	z-index: 3;
}

div.da_panelMenu4Viewer {
	position: absolute;
	left: 0px;
	top: 0px;
	width: 100%;
	height: 53px;
	margin: 0px;
	padding: 0px;
	border: 0px;
	overflow: hidden;
	z-index: 10;
}

div.da_panelMenu4ViewerLeft {
	width: 0px;
	height: 100%;
	float: left;
}

div.da_panelMenu4ViewerCenter {
	width: auto;
	height: 100%;
	overflow: hidden;
}

div.da_panelMenu4ViewerRight {
	width: 0px;
	height: 100%;
	float: right;
}

div.da_panelMenu4ViewerCenterLeft {
}

div.da_panelMenu4ViewerCenterRight {
}

div.da_panelMenu4ViewerItemLeft {
	float: left;
}

div.da_panelMenu4ViewerItemLeftTop {
	position: relative;
	top: 2px;
	width: 60px;
	height: 12px;
	font-size: 12px;
	text-align: center;
	overflow: visible;
	white-space: nowrap;
}

div.da_panelMenu4ViewerItemLeftMiddle {
	position: relative;
	top: -13px;
	width: 60px;
	height: 41px;
	font-size: 12px;
	text-align: center;
	cursor: pointer;
}

div.da_panelMenu4ViewerItemLeftBottom {
	position: relative;
	top: -18px;
	width: 60px;
	height: 14px;
	font-size: 12px;
	text-align: center;
	cursor: pointer;
}

div.da_panelMenu4ViewerItemRight {
	float: right;
}

div.da_panelMenu4ViewerItemRightTop {
	position: relative;
	top: 2px;
	width: 60px;
	height: 12px;
	font-size: 12px;
	text-align: center;
	overflow: visible;
	white-space: nowrap;
}

div.da_panelMenu4ViewerItemRightMiddle {
	position: relative;
	top: -13px;
	width: 60px;
	height: 41px;
	font-size: 12px;
	text-align: center;
	cursor: pointer;
}

div.da_panelMenu4ViewerItemRightBottom {
	position: relative;
	top: -18px;
	width: 60px;
	height: 14px;
	font-size: 12px;
	text-align: center;
	cursor: pointer;
}

div.da_panelMenu4ViewerItemPopy {
     float: right;
}

div.da_panelMenu4ViewerItemPopyTop {
	position: relative;
	top: 2px;
	width: 126px;
	height: 12px;
	font-size: 12px;
	text-align: center;
	overflow: visible;
	white-space: nowrap;
}

div.da_panelMenu4ViewerItemPopyMiddle {
	position: absolute;
	top: 0px;
	width: 126px;
	height: 53px;
	font-size: 12px;
	text-align: center;
}

div.da_panelMenu4ViewerItemPopyBottom {
	position: relative;
	top: 0px;
	width: 126px;
	height: 67px;/* 14 + 53*/ 
	font-size: 12px;
	text-align: center;
}

div.da_panelMenu4ViewerItemHidden {
	visibility: hidden;
	cursor: default;
}

div.da_panelMenu4ViewerItemGray {
	color: gray;
}

div.da_panelMenu4ViewerItemNoPointer {
	cursor: default;
}

div.da_topPanel4ViewerPulldownMenu {

    background-color:#ffffff;
    border:solid 1px #9dbaca;
    border-width:1px 1px 0px 1px;
    
}

div.da_topPanel4ViewerPulldownMenu ul {

    border:solid 1px #9dbaca;
    border-width:0px 0px 1px 0px;
    
}

div.da_topPanel4ViewerPulldownMenu li {
 
    border:solid 1px #ffffff;
	background-color: #ebf1f4;
    border-width:1px 1px 0 1px;
	
}

div.da_topPanel4ViewerPulldownMenu li.selected {

    background-color:#c6d7e0;

}
