div.da_topTitle {
	position: absolute;
	left: 0px;
	top: 0px;
	width: 100%;
	height: 47px;
	margin: 0px;
	padding: 0px;
	border: 0px;
}

div.da_topTitleLeft {
	position: absolute;
	top: 0px;
	left: 0px;
	width: 100px;
	height: 100%;
	background-repeat: no-repeat;
	background-position: 0px 0px;
	z-index: 2;
}

div.da_topTitleCenter {
	position: absolute;
	top: 0px;
	left: 0px;
	width: 99%;
	height: 100%;
	background-repeat: repeat-x;
	background-position: 0px 0px;
	z-index: 1;
}

div.da_topTitleRight {
	position: absolute;
	top: 0px;
	right: 0px;
	width: 0px;
	height: 100%;
	background-repeat: no-repeat;
	background-position: 0px 0px;
	z-index: 3;
}

div.da_topTitleLeftDiv {
	position: absolute;
	top: 8px;
	left: 10px;
    width: 100%;
	font-size: 12px;
	font-weight: bold;
	vertical-align: middle;
}

div.da_titleMenu {
	position: absolute;
	left: 0px;
	top: 0px;
	width: 100%;
	height: 47px;
	margin: 0px;
	padding: 0px;
	border: 0px;
	overflow: hidden;
	z-index: 10;
}

div.da_titleMenuLeft {
	width: 100px;
	height: 100%;
	float: left;
}

div.da_titleMenuCenter {
	width: auto;
	height: 100%;
	overflow: hidden;
}

div.da_titleMenuRight {
	width: 0px;
	height: 100%;
	float: right;
}

div.da_titleMenuCenterLeft {
}

div.da_titleMenuCenterRight {
}

div.da_titleMenuItemLeft {
	float: left;
}

div.da_titleMenuItemRight {
	float: right;
}

div.da_titleMenuItemLogo {
	float: right;
}

div.da_titleMenuItemLogoTop {
	position: relative;
	top: 0px;
	width: 80px;
	height: 12px;
	font-size: 12px;
	text-align: center;
}

div.da_titleMenuItemLogoMiddle {
	position: absolute;
	top: 0px;
	width: 80px;
	height: 34px;
	font-size: 12px;
	text-align: center;
}

div.da_titleMenuItemLogoBottom {
	position: relative;
	top: 0px;
	width: 80px;
	height: 34px;
	font-size: 12px;
	text-align: center;
}

div.da_titleMenuItemHidden {
	visibility: hidden;
	cursor: default;
}

div.da_titleMenuItemGray {
	color: gray;
}

div.da_titleMenuItemNoPointer {
	cursor: default;
}


